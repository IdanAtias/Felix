{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0",
  "id": "MICROSOFT-APP-ID",
  "packageName": "com.idan.package",
  "developer": {
    "name": "Idan Atias",
    "websiteUrl": "https://github.com/IdanAtias/Felix",
    "privacyUrl": "https://example.azurewebsites.net/privacy",
    "termsOfUseUrl": "https://example.azurewebsites.net/termsofuse"
  },
  "name": {
    "short": "Felix",
    "full": "Felix"
  },
  "description": {
    "short": "Felix helps you monitor your cloud resources",
    "full": "Felix helps you monitor your cloud resources."
  },
  "icons": {
    "outline": "icon-outline.png",
    "color": "icon-color.png"
  },
  "accentColor": "#abcdef",
  "bots": [
    {
      "botId": "MICROSOFT-APP-ID",
      "scopes": [
        "personal"
      ],
      "supportsFiles": true
    }
  ],
  "validDomains": [
    "*.azurewebsites.net",
    "*.botframework.com"
  ]
}
